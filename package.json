{
  "name": "@mkas3/eslint",
  "type": "module",
  "version": "3.0.11",
  "description": "Eslint configs",
  "author": {
    "name": "mkas3 ðŸ“›",
    "url": "https://github.com/mkas3"
  },
  "license": "MIT",
  "homepage": "https://github.com/mkas3/eslint",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkas3/eslint"
  },
  "bugs": {
    "url": "https://github.com/mkas3/eslint/issues"
  },
  "keywords": [
    "config",
    "eslint",
    "eslint-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/configs/index.js",
  "types": "dist/configs/index.d.ts",
  "files": [
    "dist/configs"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -p tsconfig.json",
    "prepublishOnly": "pnpm build",
    "dev": "npx @eslint/config-inspector@latest --config eslint.config.ts",
    "format": "prettier --write \"*.js\"",
    "prepare": "lint-staged"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "eslint": "^9.9.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@eslint-react/eslint-plugin": "^1.10.0",
    "@eslint/compat": "^1.1.1",
    "@next/eslint-plugin-next": "^14.2.5",
    "eslint": "^9.9.0",
    "eslint-flat-config-utils": "^0.3.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-readable-tailwind": "^1.7.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "jiti": "^1.21.6"
  },
  "devDependencies": {
    "lint-staged": "^15.2.9",
    "shx": "^0.3.4",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.ts": "eslint . --flag unstable_ts_config --config eslint.config.ts"
  }
}
